
@{
    ViewBag.Title = "Users";
}

<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Number</th>
        <th>Password</th>
    </tr>

    @for (int i = 0; i < ViewBag.Users.Count; i++)
    {
        <tr>
            <td>@ViewBag.Users[i].FirstName</td>
            <td>@ViewBag.Users[i].LastName</td>
            <td>@ViewBag.Users[i].Email</td>
            <td>@ViewBag.Users[i].Number</td>
            <td>@ViewBag.Users[i].Password</td>
        </tr>
    }
</table>

